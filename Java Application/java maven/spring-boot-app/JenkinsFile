pipeline {
    agent any
  tools{
      jdk 'Java17'
      maven 'Maven3'
  }
    stages {
        stage('clean up workspace') {
            steps {
                cleanWs()
                echo "clean is  complete"
            }
        }
        stage('Checkout from Git'){
            steps {
                git branch: 'main',credentialsId: 'github',url:'https://github.com/sunil3gs98/Test/Java Application/java maven/spring-boot-app'
                echo "Git checkout Is complete"
            }
        }
        
        
    }
}
